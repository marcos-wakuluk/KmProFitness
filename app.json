{
  "expo": {
    "name": "KmProFitness",
    "slug": "KmProFitness",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "48.0.0",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "googleSignIn": {
          "reservedClientId": "TU_ID_DE_CLIENTE_IOS"
        }
      },
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleSignIn": {
          "clientId": ""
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"

      },
      "package":"com.kmprofitness"
    },
    "web": {
      "config": {
        "googleSignIn": {
          "clientId": "TU_ID_DE_CLIENTE_ANDROID"
        }
      },
      "favicon": "./assets/favicon.png"
      
    },
     "scheme": "kmprofitness"
  }
}
